 $filter = @{
        Logname = "Security"
        ID      = 4624, 4634
        StartTime = (Get-Date).AddHours(-12)
    }
    Get-WinEvent -FilterHashtable $filter | Select-Object -Property UserId -First 1

    Get-WinEvent -FilterHashtable $filter | Select-Object -Property * | Export-Csv -Path C:\ClassFiles-JB\winevent.csv -NoTypeInformation

    Get-WinEvent -FilterHashtable $filter | Select-Object -ExpandProperty Properties | Out-GridView

    Get-winevent -FilterHashtable $filter | Select-Object -ExpandProperty Properties[5].Value



    